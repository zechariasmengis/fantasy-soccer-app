<%= form_for @team do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :primary_color %>
    <%= f.text_field :primary_color %><br>

    <%= f.label :secondary_color %>
    <%= f.text_field :secondary_color %><br>

    <%= f.label :user_id %>
    <%= f.collection_select :user_id, @users, :id, :name %><br>

    <%= f.submit %>
<% end %><br>

